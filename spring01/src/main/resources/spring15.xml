<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
      http://www.springframework.org/schema/context
      http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:annotation-config/>

    <bean id="user" class="com.msj.demo15.User"></bean>

    <!--有两个相同类型的bean-->
    <bean id="student1" class="com.msj.demo15.Student">
        <property name="sname" value="msj"></property>
        <property name="age" value="12"></property>
    </bean>

    <bean id="student2" class="com.msj.demo15.Student">
        <property name="sname" value="baibai"></property>
        <property name="age" value="13"></property>
    </bean>
</beans>